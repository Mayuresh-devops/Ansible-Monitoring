[Unit]
Description=Grafana
Documentation=https://grafana.com/docs/grafana/latest/
After=network.target

[Service]
Type=simple
User=ubuntu
ExecStart=/home/ubuntu/grafana-v11.1.3/bin/grafana-server --config=/home/ubuntu/grafana-v11.1.3/conf/defaults.ini
Restart=always
WorkingDirectory=/home/ubuntu/grafana-v11.1.3

[Install]
WantedBy=multi-user.target
